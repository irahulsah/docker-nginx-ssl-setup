{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "irahulsah/dkr-multicontainer-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["reactclient", "apibackend"],
        "memory": 100
      },
      {
        "name": "reactclient",
        "image": "irahulsah/multicontainer-react_frontend",
        "hostname": "reactclient",
        "essential": false,
        "memory": 300
      },
      {
        "name": "apibackend",
        "image": "irahulsah/multicontainer-api_backend",
        "hostname": "apibackend",
        "essential": false,
        "memory": 300
      }
    ]
}